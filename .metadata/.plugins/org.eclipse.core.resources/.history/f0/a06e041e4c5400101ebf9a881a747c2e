package com.java.cal;

public class Calculators {
	
	public static void main(String[] args) {
		
//		calculator
//		add , sub, mul, div
		
		System.out.println(add(1,2));
		
	}
//	add -> a, b ,,=>a+b sum
	public static int add(int a , int b) {
//		int sum=0;
//		sum=a+b;
//		return sum;
		return a+b;
	}
	
	

}
